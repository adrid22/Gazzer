{
  "expo": {
    "name": "gazzer",
    "slug": "gazzer",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8A4AF3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.filipsapps.gazzer",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to send images in the chat."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#8A4AF3"
      },
      "package": "com.filipsapps.gazzer",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "gazzer",
    "extra": {
      "eas": {
        "projectId": "gazzer-1a03c"
      }
    },
    "newArchEnabled": true
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
    "@expo/metro-config": "~0.19.0",
    "@expo/prebuild-config": "~8.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/stack": "6.3.29",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "~52.0.36",
    "expo-auth-session": "~6.0.3",
    "expo-dev-client": "~5.0.12",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-localization": "~16.0.1",
    "expo-location": "~18.0.7",
    "expo-modules-autolinking": "~2.0.0",
    "expo-notifications": "~0.29.13",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-vector-icons": "^10.0.1",
    "expo-web-browser": "~14.0.2",
    "firebase": "~10.12.2",
    "i18n-js": "~4.4.3",
    "metro": "0.81.0",
    "metro-config": "0.81.0",
    "metro-resolver": "0.81.0",
    "nanoid": "3.3.7",
    "react": "~18.3.1",
    "react-native": "~0.76.7",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-maps": "1.18.0",
    "react-native-maps-directions": "^1.9.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "~4.9.0",
    "react-native-screens": "~3.31.1",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "resolutions": {
    "nanoid": "3.3.7"
  },
  "name": "gazzer",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/adrid22/Gazzer",
  "author": "Adrid22 <adridlol22@gmail.com>",
  "license": "MIT"
}
